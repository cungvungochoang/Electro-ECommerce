@model IEnumerable<QuanLyCuaHangGamingGear.Models.HangSx>

<div class="aside">
	<h3 class="aside-title">Thương hiệu</h3>
	<div class="checkbox-filter">
		@foreach (var item in Model)
		{
			<div class="input-checkbox">
				<input type="checkbox" id="@item.TenHangSx">
				<label for="@item.TenHangSx">
					<span></span>
					@item.TenHangSx
				</label>
			</div>
		}
	</div>
</div>